        "for a_val, color in zip(a_values, colors):\n",
        "    y_approx = np.array([fourier_truncada(x, a_val) for x in x_vals])\n",
        "    plt.plot(x_vals, y_approx, label=f'Aproximación a={a_val}', linestyle='--', color=color)\n",
