        "\n",
        "import sympy as sp\n",
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# Definir símbolos\n",
        "x, omega, a = sp.symbols('x omega a')\n",
        "\n",
        "# a) Gráfico de la función pulso unitario\n",
        "def pulse_function(x_val):\n",
        "    return 1 if -1 <= x_val <= 1 else 0\n",
        "\n",
        "x_vals = np.linspace(-3, 3, 1000)\n",
        "y_vals = np.array([pulse_function(x) for x in x_vals])\n",
        "\n",
        "plt.figure(figsize=(12, 8))"
      ]
